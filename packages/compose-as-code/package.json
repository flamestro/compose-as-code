{
  "name": "compose-as-code",
  "version": "1.4.5",
  "description": "Provides a module to express docker compose files as code",
  "main": "./lib/cli.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ]
    }
  },
  "jest": {
    "testTimeout": 10000
  },
  "scripts": {
    "test": "NODE_ENV=test jest --testMatch='**/*.test.ts'",
    "lint": "eslint . --ext .ts",
    "format:check": "prettier --ignore-path .gitignore --check \"**/*.+(js|ts|json)\"",
    "format:fix": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flamestro/compose-as-code.git"
  },
  "bin": {
    "conduct": "./lib/cli.js"
  },
  "keywords": [
    "docker-compose",
    "iac",
    "docker",
    "as",
    "code",
    "compose",
    "as",
    "code"
  ],
  "author": "Ahmet Kilic @flamestro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flamestro/compose-as-code/issues"
  },
  "homepage": "https://github.com/flamestro/compose-as-code#readme",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.43.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@types/node": "^20.2.1",
    "esbuild": "^0.17.19"
  },
  "gitHead": "fe680bdbd4d75a29ad005143c38c65fdd3b3b264"
}
